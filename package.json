{
  "name": "webpack-babel-server",
  "version": "0.0.4",
  "description": "Webpack Babel Starter Kit",
  "main": "''",
  "engines": {
    "node": "10.12.0"
  },
  "scripts": {
    "clean": "shx rm -rf build",
    "webpack": "webpack --config config/webpack.config.production.js",
    "zip": "node nodeScripts/convertToEs5.js",
    "unzip": "node nodeScripts/decompress.js",
    "sass": "node-sass src/styles/index.scss build/index.css --output-style compressed",
    "build": "npm run clean && npm run webpack",
    "lint": "eslint src config",
    "dev": " webpack-dev-server --config config/webpack.config.dev.js",
    "test": "export NODE_ENV=test && jest",
    "start": "serve -s build"
  },
  "author": "ReactJSResources",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-polyfill": "6.26.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-jest": "^23.1.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss-cli": "^6.1.3",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.14.0",
    "serve": "^11.2.0",
    "shx": "^0.3.2",
    "style-loader": "^1.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-loader": "^0.5.5",
    "compressing": "^1.5.0"
  }
}
